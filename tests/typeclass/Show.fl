# STDOUT == null\n
# DESCRIPTION == Custom `Show` interface 

class MyShow [T] {
    fun my_to_string(T) String
}

rec Test = Null

instance MyShow [Test] {
    fun my_to_string(self) {
        "null"
    }
}

fun my_print[T: MyShow] (val: T) {
    print(val.my_to_string())
}

fun entry () {
    my_print(Test.Null)
}
