-- recursive with no mutation
let fib = (n1: i32, n2: i32) -> (i32, i32) {
    -- std::fmt::print(n2);
    return fib(n2, n1);
};

let entry = () -> i32 {
    let x = 10;
    let myfoo = () -> (i32, i32) {
        yield (x, 10);
    };
    return x;
};
