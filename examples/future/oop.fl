-- Patterns are like traits and interfaces
pattern Fart {
    def fart(&mut this);
}

struct Cow {
    let farts: int,
    let weight: kg,
}

type kg = int;

impl Cow {
    def new(weight: kg) -> Animal {
        return Animal {
            farts = 0, 
            weight = weight
        };
    }
}

def entry() {
    let mut cow: Cow = Cow(10);
    cow.fart()
}

