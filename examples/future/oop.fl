-- Patterns are like traits and interfaces
pattern Fart {
    let fart = (&mut this);
}

struct Cow {
    let farts: int,
    let weight: kg,
}

type kg = int;

impl Cow {
    let new = (weight: kg) -> Animal {
        return Animal {
            farts = 0, 
            weight = weight
        };
    };
}

let entry = () {
    let mut cow: Cow = Cow(10);
    cow.fart()
};

