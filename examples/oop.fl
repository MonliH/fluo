struct Animal {
    let farts: int,
    let weight: int,
}

type kg = int;

impl Animal {
    def fart(&mut this) {
        this.farts += 1;
    }
    def new(weight: kg) {
        return Animal { farts = 0, weight = weight };
    }
}

def entry() {
    let mut cow: Animal = Animal(10);
    cow.fart()
}

